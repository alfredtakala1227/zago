# Use PHP5.4 as default
AddHandler application/x-httpd-php54 .php

Options +FollowSymLinks

RewriteEngine On
RewriteBase /
RewriteCond %{HTTP_HOST} !^new.zagollc.com$ [NC]
RewriteRule ^(.*)$ new.zagollc.com/$1 [L,R=301]



